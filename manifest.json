{

  "manifest_version": 2,
  "name": "Remind Em",
  "description": "Reminder",
  "version": "1.0",
  "icons": {
    "48": "images/icon32.jpeg"
  },
"browser_action": {
  "default_popup": "popup/popup.html",
  "default_title": "Symbols & Accents Toolkit",
  "default_icon": {
    "48": "images/icon32.jpeg"
  }
},

"permissions": ["storage","notifications"],

  "background": {
    "scripts": ["background-script.js"],
    "persistent": true
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
